@model IEnumerable<Proyecto_BD.Models.Consultorio>

@{
    ViewData["Title"] = "Index";
}

<h1>Consultorios</h1>

<p>
    <a asp-action="AsignarConsultorio" class="btn btn-primary">Asignar Consultorio</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Piso)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Numero_Consultorio)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Piso)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Numero_Consultorio)
            </td>
        </tr>
}
    </tbody>
</table>
